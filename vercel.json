{
    "version": 2,
    "framework": null,
    "functions":{
          "api/*.php": {
          "runtime": "vercel-php@0.6.0"
    } 
    },
   
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "PHP_VERSION": "8.2.0",
      "APP_ENV": "production",
      "APP_DEBUG": "true",
      "APP_URL": "https://your-domain.vercel.app",
      "DB_CONNECTION": "mysql",
      "DB_HOST": "db",
      "DB_PORT": "3306",
      "DB_DATABASE": "mydb",
      "DB_USERNAME": "root",
      "DB_PASSWORD": "password"
    }
  }
  